{
  "rewrites": [
    {
      "source": "/",
      "destination": "/dashboard/welcome",
      "has": [
        {
          "type": "host",
          "value": "dashboard.thestreetlamp.com"
        }
      ]
    },
    {
      "source": "/",
      "destination": "/admin",
      "has": [
        {
          "type": "host",
          "value": "admin.thestreetlamp.com"
        }
      ]
    },
    {
      "source": "/",
      "destination": "/artist",
      "has": [
        {
          "type": "host",
          "value": "artist.thestreetlamp.com"
        }
      ]
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
} 