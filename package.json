    "supabase:link": "supabase link --project-ref ldmppmnpgdxueebkkpid",
    "migration:phase3-phase4": "node scripts/apply-phase3-phase4-migration.js",
    "test:graphql": "jest --testNamePattern='GraphQL'",
    "test:auth": "jest --testNamePattern='Authentication'",
    "test:performance": "artillery run performance-tests.yml",
    "security-audit": "npm audit",
    "prepare": "husky",
    "ui:find-usage": "node scripts/find-component-usage.js",
    "ui:migration-status": "node scripts/ui-migration-status.js",
    "polaris:check-updates": "npm outdated @shopify/polaris @shopify/polaris-icons @shopify/polaris-tokens",
    "polaris:health": "npm list @shopify/polaris @shopify/polaris-icons @shopify/polaris-tokens",
    "clear-cache": "node scripts/clear-build-cache.js",
    "fix:duplicates": "tsx scripts/merge-duplicate-collector-profiles.ts",
    "fix:duplicates:execute": "tsx scripts/merge-duplicate-collector-profiles.ts --execute",
    "verify:products": "tsx scripts/verify-product-handles.ts",
    "sync:shopify": "tsx scripts/sync-product-handles-from-shopify.ts",
    "update:barcodes": "tsx scripts/update-shopify-barcodes.js",
    "register:webhooks": "tsx scripts/register-shopify-product-webhook.js",
    "process:barcodes": "curl -X POST http://localhost:3000/api/admin/process-all-barcodes -H \"Content-Type: application/json\" -d '{\"limit\": 100}'",
    "test:webhooks": "tsx scripts/test-barcode-webhooks.js"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "node scripts/scan-secrets.js"
    ],
    "vercel.json": [
      "node scripts/scan-secrets.js"
    ],
    ".env*": [
      "node scripts/scan-secrets.js"
    ]
