  "name": "coa-service",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "typecheck:collector": "tsc -p tsconfig.collector.json --noEmit",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "backup:build": "tsc -p tsconfig.backup.json",
    "backup:start": "node backup/dist/scripts/backup-database.js",
    "backup:test": "node backup/dist/scripts/test-backup.js",
    "backup:schedule": "node backup/dist/scripts/schedule-backup.js",
