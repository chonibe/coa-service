    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "supabase:enable-google": "node scripts/enable-google-provider.js",
    "supabase:google-redirect-uri": "node -r dotenv/config scripts/print-google-redirect-uri.js",
    "setup:ga4": "node scripts/setup-ga4-insights.js",
    "setup:ga4-manual": "node scripts/setup-ga4-manual.js",
    "validate:ga4": "node scripts/validate-ga4-setup.js",
