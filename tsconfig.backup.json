{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./backup/dist",
    "rootDir": "./backup",
    "baseUrl": "./backup",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "backup/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "backup/dist",
    "lib/**/*"
  ]
} 